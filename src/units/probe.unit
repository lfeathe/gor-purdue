    #attack can be seperated attack units and attack towers
    - id: 23
      name: attack #attack unit
      require:
        - target: pointorunit

    - id: 24
      name: attack #attack towers
      require:
        - target: point

    #according to building ID, action id id diff(880~910)
    - id: 880 
      name: protossbuild
      require:
        - target: point
        - building: int #building ID
        - location_x: int
