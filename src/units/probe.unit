      damage: 5.0
      attacks: 1
      range: 0.10009765625
      speed: 1.5
  mineral_cost: 50
  vespene_cost: 0
  actions:
    - id: 4
      name: stop
      require:
        - target: none

    - id: 16
      name: move
      require:
